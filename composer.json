{
  "name": "joker/laravel-aliyun-amqp",
  "description": "joker/laravel-aliyun-amqp",
  "license": "MIT",
  "authors": [
    {
      "name": "Joker.Cao",
      "email": "yuanxing008@vip.qq.com"
    }
  ],
  "require": {
    "php": "7.*",
    "php-amqplib/php-amqplib": ">=2.7 <3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*|6.*",
    "squizlabs/php_codesniffer": "2.*",
    "phpmd/phpmd": "@stable",
    "laravel/framework": "5.8"
  },
  "autoload": {
    "psr-4": {
      "Joker\\LaravelAliyunAmqp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Joker\\LaravelAliyunAmqp\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Joker\\LaravelAliyunAmqp\\Providers\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
